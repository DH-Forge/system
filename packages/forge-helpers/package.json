{
	"name": "@forge/helpers",
	"description": "DHFS Helpers for resolving and formatting data",
	"version": "0.0.0",
	"main": "./src/index.ts",
	"module": "./src/index.ts",
	"types": "./src/index.ts",
	"sideEffects": false,
	"license": "MIT",
	"files": ["src/**"],
	"exports": {
		".": "./src/index.ts",
		"./version": "./src/version/index.ts",
		"./version/*": "./src/version/*"
	},
	"scripts": {
		"clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
		"check": "tsc --noEmit"
	},
	"devDependencies": {
		"@forge/tsconfig": "workspace:*",
		"typescript": "5.8.3"
	},
	"dependencies": {
		"zod": "^3.25.32"
	},
	"publishConfig": {
		"access": "public"
	}
}
